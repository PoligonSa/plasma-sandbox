    "@commitlint/config-conventional": "17.4.4",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "auto": "10.36.6",
    "chalk": "4.1.2",
    "commander": "10.0.0",
    "eslint": "^8.46.0",
    "eslint-plugin-react": "^7.33.1",
    "fs-extra": "10.0.0",
    "http-server": "0.12.3",
    "husky": "^8.0.0",
    "lerna": "6.5.1",
    "lint-staged": "13.1.2",
    "node-fetch": "2.6.1",
